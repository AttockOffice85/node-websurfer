{
  "name": "websurfer",
  "version": "1.0.0",
  "description": "a surffing application to crawl into linkedin and other social platform and interact with posts and sections",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "concurrently \"npm run start:bot\" \"npm run start:server\"",
    "start:bot": "node dist/index.js",
    "start:server": "node dist/logServer.js",
    "dev": "concurrently \"npm run dev:bot\" \"npm run dev:server\"",
    "dev:bot": "ts-node ./index.ts",
    "dev:server": "ts-node-dev --respawn ./src/server.ts",
    "build:watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AttockOffice85/node-websurfer.git"
  },
  "keywords": [
    "webserver",
    "social",
    "media",
    "node",
    "js",
    "puppeteer"
  ],
  "author": "Bakhtiar@Synet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AttockOffice85/node-websurfer/issues"
  },
  "homepage": "https://github.com/AttockOffice85/node-websurfer#readme",
  "dependencies": {
    "@types/express": "^5.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "puppeteer": "^23.4.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/puppeteer": "^5.4.7",
    "concurrently": "^9.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
